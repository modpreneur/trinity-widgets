{
  "name": "trinity/widgets",
  "description": "Trinity Widgets bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Modpreneur Team",
      "email": "info@modpreneur.com"
    },
    {
      "name": "Richard Bureš",
