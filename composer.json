{
  "name": "trinity/widgets",
  "description": "Trinity Widgets bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Trinity",
      "email": "info@modpreneur.com"
    }
  ],
  "require": {
    "php": ">=7",
    "symfony/symfony": "~2.8 | ~3.0",
    "nette/utils": "~2.3",
    "sensio/framework-extra-bundle": "~3.0",
    "twig/extensions": "~1.3",
    "trinity/framework": "dev-master",
    "friendsofsymfony/user-bundle": "dev-master",
    "trinity/settings": "dev-master",
    "trinity/logger": "dev-master"
  },
  "require-dev": {
    "braincrafted/testing-bundle": "@stable",
    "phpunit/phpunit": "~5",
    "doctrine/data-fixtures": "~1",
    "doctrine/doctrine-fixtures-bundle": "~2"
  },


  "autoload": {
    "psr-4": { "Trinity\\WidgetsBundle\\": "WidgetsBundle" },
    "exclude-from-classmap": [
      "/WidgetsBundle/Tests/"
    ]
  },


  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}


